{
	"name": "@tuplo/use-combo-box",
	"description": "Headless combo box hook for React",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/use-combo-box.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [
		"react",
		"react hooks",
		"combo box",
		"headless ui",
		"WAI-ARIA"
	],
	"types": "dist/index.d.ts",
	"module": "./dist/index.mjs",
	"main": "./dist/index.cjs",
	"exports": {
		".": [
			{
				"import": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.mjs"
				},
				"require": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.cjs"
				},
				"default": "./dist/index.mjs"
			},
			"./dist/index.mjs"
		]
	},
	"files": [
		"dist/use-combo-box.d.ts",
		"dist/index.cjs",
		"dist/index.d.ts",
		"dist/index.mjs"
	],
	"scripts": {
		"build": "tsx sh/build.ts",
		"coverage": "tsx sh/coverage.ts",
		"format": "prettier --write src sh",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint src --ext ts,tsx",
		"test:ci": "IS_REACT_ACT_ENVIRONMENT=true jest --ci",
		"test": "IS_REACT_ACT_ENVIRONMENT=true jest --watch"
	},
	"peerDependencies": {
		"react": "^16.8.6 || ^17.0.0 || ^18.0.0"
	},
	"devDependencies": {
		"@testing-library/dom": "9.3.3",
		"@testing-library/jest-dom": "6.1.4",
		"@testing-library/react": "14.0.0",
		"@testing-library/user-event": "14.5.1",
		"@tuplo/shell": "1.2.1",
		"@types/jest": "29.5.6",
		"@typescript-eslint/eslint-plugin": "6.9.0",
		"@typescript-eslint/parser": "6.9.0",
		"esbuild": "0.19.5",
		"eslint": "8.52.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "9.0.0",
		"eslint-config-react-app": "7.0.1",
		"eslint-plugin-import": "2.29.0",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"nyc": "15.1.0",
		"prettier": "3.0.3",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"ts-jest": "29.1.1",
		"tsx": "3.14.0",
		"typescript": "5.2.2"
	}
}
