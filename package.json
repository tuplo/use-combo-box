{
	"name": "@tuplo/use-combo-box",
	"description": "Headless combo box hook for React",
	"version": "0.0.0-development",
	"repository": "git@github.com:tuplo/use-combo-box.git",
	"author": "Rui Costa",
	"license": "MIT",
	"keywords": [
		"react",
		"react hooks",
		"combo box",
		"headless ui",
		"WAI-ARIA"
	],
	"types": "dist/index.d.ts",
	"module": "./dist/index.mjs",
	"main": "./dist/index.cjs",
	"exports": {
		".": [
			{
				"import": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.mjs"
				},
				"require": {
					"types": "./dist/index.d.ts",
					"default": "./dist/index.cjs"
				},
				"default": "./dist/index.mjs"
			},
			"./dist/index.mjs"
		]
	},
	"files": [
		"dist/use-combo-box.d.ts",
		"dist/index.cjs",
		"dist/index.d.ts",
		"dist/index.mjs"
	],
	"scripts": {
		"build": "tsx sh/build.ts",
		"coverage": "tsx sh/coverage.ts",
		"format": "prettier --write src sh",
		"lint:ts": "tsc --noEmit",
		"lint": "eslint sh/ src/",
		"test:ci": "IS_REACT_ACT_ENVIRONMENT=true jest --ci",
		"test": "IS_REACT_ACT_ENVIRONMENT=true jest --watch",
		"upgrade": "npm-check-updates -u && npm install"
	},
	"peerDependencies": {
		"react": "^16.8.6 || ^17.0.0 || ^18.0.0 || ^19.0.0"
	},
	"devDependencies": {
		"@testing-library/dom": "10.4.0",
		"@testing-library/jest-dom": "6.6.3",
		"@testing-library/react": "16.1.0",
		"@testing-library/user-event": "14.5.2",
		"@tuplo/shell": "1.2.3",
		"@types/jest": "29.5.14",
		"esbuild": "0.24.2",
		"eslint": "9.17.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-unicorn": "56.0.1",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"npm-check-updates": "17.1.13",
		"nyc": "17.1.0",
		"prettier": "3.4.2",
		"react": "19.0.0",
		"react-dom": "19.0.0",
		"ts-jest": "29.2.5",
		"tsx": "4.19.2",
		"typescript": "5.7.2",
		"typescript-eslint": "8.19.0"
	},
	"dependencies": {
		"@types/react": "19.0.2",
		"@types/react-dom": "19.0.2",
		"eslint-plugin-react": "7.37.3",
		"eslint-plugin-react-hooks": "5.1.0",
		"eslint-plugin-vitest": "0.5.4"
	}
}
